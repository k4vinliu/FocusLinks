{
  "name": "url-saver-chrome-extension",
  "version": "0.0.1",
  "description": "A simple Chrome Extension allowing users to save and delete links with just one click",
  "main": "index.js",
  "scripts": {
    "dev": "npx mix",
    "postdev": "npx tailwindcss -i ./src/css/popup.css -o ./dist/css/popup.css",
    "prod": "npx mix --production",
    "postprod": "npx tailwindcss -i ./src/css/popup.css -o ./dist/css/popup.css -m"
  },
  "keywords": [],
  "author": "Kevin Liu",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "laravel-mix": "^6.0.49",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "vue-loader": "^16.8.3"
  },
  "dependencies": {
    "vue": "^3.4.24"
  }
}
